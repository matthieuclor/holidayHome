if ($("#booking-approval-modal").length > 0) {
  $("#booking-approval-modal").modal("hide")
}

$("#modal-container").html("<%= j render "form" %>")

<% if flash.present? %>
  $("#toast-container").prepend("<%= j render "shared/flash_messages" %>")
  <% flash.clear %>
<% end %>

$("#booking-approval-modal").modal("show")
