if ($("#edit-account").length > 0) {
  $("#edit-account").html("<%= j render "edit" %>")

  $('input[type="file"]').change((e) => {
    $('.custom-file-label').html(e.target.files[0].name);
  });
}

if ($("#edit-password").length > 0) {
  $("#edit-password").html("<%= j render "edit_password" %>")
}

if ($("#deactivate-account").length > 0) {
  $("#deactivate-account").html("<%= j render "deactivate_account" %>")
}
